{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1, 
  "newProjectRoot": "projects",
  "projects": {
    "@gummelhummel/bulma-page-loader": {
      "projectType": "library",
      "root": "projects/gummelhummel/bulma-page-loader",
      "sourceRoot": "projects/gummelhummel/bulma-page-loader/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/gummelhummel/bulma-page-loader/tsconfig.lib.json",
            "project": "projects/gummelhummel/bulma-page-loader/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/gummelhummel/bulma-page-loader/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/gummelhummel/bulma-page-loader/src/test.ts",
            "tsConfig": "projects/gummelhummel/bulma-page-loader/tsconfig.spec.json",
            "karmaConfig": "projects/gummelhummel/bulma-page-loader/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/gummelhummel/bulma-page-loader/tsconfig.lib.json",
              "projects/gummelhummel/bulma-page-loader/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "@gummelhummel/bulma-toast": {
      "projectType": "library",
      "root": "projects/gummelhummel/bulma-toast",
      "sourceRoot": "projects/gummelhummel/bulma-toast/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/gummelhummel/bulma-toast/tsconfig.lib.json",
            "project": "projects/gummelhummel/bulma-toast/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/gummelhummel/bulma-toast/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/gummelhummel/bulma-toast/src/test.ts",
            "tsConfig": "projects/gummelhummel/bulma-toast/tsconfig.spec.json",
            "karmaConfig": "projects/gummelhummel/bulma-toast/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/gummelhummel/bulma-toast/tsconfig.lib.json",
              "projects/gummelhummel/bulma-toast/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }},
  "defaultProject": "@gummelhummel/bulma-page-loader"
}
